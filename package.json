{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "webpack template for projects",
  "main": "index.js",
  "private": true,
  "scripts": {
    "eslint:fix": "eslint ./src --fix",
    "webpack": "cross-env NODE_ENV=production node -r esm \"./node_modules/.bin/webpack\" --config=\"./webpack/config/webpack.common.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylerdurden10/webpack-template.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Tyler Durden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tylerdurden10/webpack-template/issues"
  },
  "homepage": "https://github.com/tylerdurden10/webpack-template#readme",
  "devDependencies": {
    "app-root-path": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "esm": "^3.2.25",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn run eslint:fix"
    ]
  }
}
